@model SistemaControleVendas.Models.NivelAcesso

@{
    ViewData["Title"] = "Create";
}

<h1>Cadastrar Nível de Acesso</h1>

<h4></h4>
<hr />
<div class="container">
            <form asp-action="Create" class="row g-3">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="col-md-6">
             <div class="form-group">
             <label asp-for="TipoUsuario" class="control-label"></label>
             <input asp-for="TipoUsuario" class="form-control" />
             <span asp-validation-for="TipoUsuario" class="text-danger"></span>
            </div>
            <div class="dropdown-divider"></div>
            <div class="form-group">
                <input type="submit" value="Cadastrar" class="btn btn-primary" />
            </div>
        </form>
    </div>
<div>
    <a asp-action="Index">Voltar</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
